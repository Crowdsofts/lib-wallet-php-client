{
    "name": "evp/paysera-wallet-php-client",
    "description": "PHP client for Paysera.com Wallet API",
    "autoload": {
        "psr-4": { "Paysera\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "rector/rector": "^0.18.4",
        "symplify/easy-coding-standard": "^12.0",
        "phpstan/phpstan": "^1.10",
        "shipmonk/phpstan-rules": "^2.9"
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "paysera/lib-wallet-transfer-rest-client": "^1.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Crowdsofts/lib-wallet-transfer-rest-client"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Crowdsofts/lib-rest-client-common"
        }
    ]
}
